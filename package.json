    "amplitude-js": "^7.2.2",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.11.0",
    "email-validator": "^2.0.4",
    "env-cmd": "^10.1.0",
    "history": "^4.10.1",
    "md5": "^2.2.1",
    "msw": "^0.19.5",
    "query-string": "^6.13.2",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-avatar": "^3.9.2",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^11.3.2",
    "react-facebook-pixel": "^1.0.3",
    "react-ga": "^3.1.2",
    "react-gtm-module": "^2.0.10",
    "react-helmet": "^6.1.0",
    "react-image-crop": "^8.5.0",
    "react-jss": "^10.3.0",
    "react-markdown": "^4.3.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "react-select": "^3.1.0",
    "react-share": "^4.3.1",
    "react-tooltip": "^4.2.9",
    "react-window": "^1.8.6",
